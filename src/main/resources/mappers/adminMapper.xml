<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.board.mappers.admin">
  
  	<!-- 회원 리스트 -->
	<select id="mlist" resultType="com.board.domain.MemberVO"> <!-- resultType 은 쿼리의 결과의 형태(DB에 있는 값을 가져올 때 쓰임) -->
		select
			member_id, member_pwd
		from member
	</select>
	
  </mapper>